{
	"presets": [
		[ "env", { "targets": { "node": 6 }, "useBuiltIns": true } ],
		"stage-0",
		"react"
    ],
	"plugins": [
		"transform-object-rest-spread",
		"transform-flow-strip-types",
		"transform-async-to-generator"
	],
	"env": {
		"test": {
			"presets": ["es2015"]
		},
		"development": {
			"plugins": [ "react-hot-loader/babel" ],
			"presets": [ "react-hmre" ]
		}
	}
}
